<html>
  <head>
    <title>{{title}}</title>
  </head>
  <body>
    <can-import from="example-app/styles.less" />
    <can-import from="example-app/app" export-as="viewModel" />
    <can-import from="example-app/account-list/" />

    <h1>{{message}}</h1>

    <account-list></account-list>

    {{#switch env.NODE_ENV}}
      {{#case "production"}}
        <script src="{{joinBase 'node_modules/steal/steal.production.js'}}"  main="example-app/index.stache!done-autorender"></script>
      {{/case}}
      {{#default}}
        <script src="/node_modules/steal/steal.js"></script>
      {{/default}}
    {{/switch}}
  </body>
</html>
